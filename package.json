        "ejs": "^2.6.1",
        "koa": "^2.5.1",
        "koa-bodyparser": "^4.2.1",
        "koa-convert": "^1.2.0",
        "koa-logger": "^3.2.0",
        "koa-mysql-session": "0.0.2",
        "koa-router": "^7.4.0",
        "koa-session-minimal": "^3.0.4",
        "koa-static": "^4.0.3",
        "koa-views": "^6.1.4",
        "mysql": "^2.15.0",
        "react": "^16.4.0",
        "react-dom": "^16.4.0",
        "validator": "^10.3.0",
        "whatwg-fetch": "^2.0.4"
    },
    "devDependencies": {
        "babel-core": "^6.26.3",
